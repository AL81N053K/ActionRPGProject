Music="*res://src/autoload/Music.tscn"
Transition="*res://src/prefabs/SceneTransitionRect.tscn"
GlobalWorldEnvironment="*res://src/autoload/WorldEnvironment.tscn"
Dir="*res://src/autoload/Directory.gd"
Inventories="*res://src/autoload/Inventories.tscn"
