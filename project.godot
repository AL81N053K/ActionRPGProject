[PluginSettings]

gdinv/ItemJsonPaths=PoolStringArray( "res://data/items/" )
gdinv/LoadOnReady=true
gdinv/RecursiveLoad=true

[application]

config/name="ActionRPGProject"
run/main_scene="res://Maps/MainMenu.tscn"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon_v1.png"

[autoload]

MainScene="*res://Preload/MainScene.gd"
GlobalWorldEnvironment="*res://Nodes/WorldEnvironment.tscn"
GlobalSettings="*res://Preload/global_settings.gd"
SceneChanger="*res://Preload/SceneChanger.gd"
PlayerStats="*res://Preload/Stats.tscn"
Save="*res://Preload/save.gd"
SaveGame="*res://Preload/SaveGame.gd"
CommandHistory="*res://Scripts/command_history.gd"
ScreenEffects="*res://Preload/ScreenEffects.tscn"
PlayerInventory="*res://Preload/PlayerInventory.tscn"
PlayerArmorInventory="*res://Preload/PlayerArmorInventory.tscn"
GDInv_ItemDB="*res://addons/gdinv/GDInv_ItemDB.gd"
Music="*res://Preload/Music.tscn"

[debug]

gdscript/completion/autocomplete_setters_and_getters=true
shapes/collision/shape_color=Color( 1, 1, 1, 0.419608 )
multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/size/width=640
window/size/height=360
window/size/resizable=false
window/size/test_width=1440
window/size/test_height=810
window/vsync/vsync_via_compositor=true
window/per_pixel_transparency/allowed=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/Checklist/plugin.cfg", "res://addons/dialogic/plugin.cfg", "res://addons/explore-editor-theme/plugin.cfg", "res://addons/gdinv/plugin.cfg", "res://addons/godot_editor_discord_presence/plugin.cfg", "res://addons/project_map/plugin.cfg", "res://addons/smoothing/plugin.cfg" )

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

[gui]

theme/use_hidpi=true
theme/custom="res://Theme stuff/main_theme.tres"
theme/custom_font="res://Theme stuff/PressStart2P.tres"

