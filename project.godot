[debug]

gdscript/completion/autocomplete_setters_and_getters=true
shapes/collision/shape_color=Color( 1, 1, 1, 0.419608 )
multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/size/width=640
window/size/height=360
window/size/resizable=false
window/size/test_width=1440
window/size/test_height=810
window/vsync/vsync_via_compositor=true
window/per_pixel_transparency/allowed=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/Checklist/plugin.cfg", "res://addons/controller_icons/plugin.cfg", "res://addons/gdinv/plugin.cfg", "res://addons/project_map/plugin.cfg" )

